# Gmail OAuth2 (Get from Google Cloud Console)
MIC_OAuth2__Gmail__ClientId=your-client-id.apps.googleusercontent.com
MIC_OAuth2__Gmail__ClientSecret=your-client-secret

# Outlook OAuth2 (Get from Azure App Registration)
MIC_OAuth2__Outlook__ClientId=your-client-id
MIC_OAuth2__Outlook__ClientSecret=your-client-secret
MIC_OAuth2__Outlook__TenantId=your-tenant-id

# OpenAI Configuration (Get from platform.openai.com)
MIC_AI__OpenAI__ApiKey=sk-your-openai-api-key-here

# Azure OpenAI Configuration (Alternative to OpenAI)
MIC_AI__AzureOpenAI__Endpoint=https://your-resource.openai.azure.com/
MIC_AI__AzureOpenAI__ApiKey=your-azure-openai-key
MIC_AI__AzureOpenAI__DeploymentName=gpt-4
